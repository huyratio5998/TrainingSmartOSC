@model IEnumerable<WebApplication1.Models.Genre>

    @{
        ViewBag.Title = "Store";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h2>Browse Genre</h2>
<p>

    Select from @Model.Count() genres: 
</p>
<ul>
    @foreach (var  genre in Model)
    {
        <li >@Html.ActionLink(genre.Name,"Browse",new { genre = genre.Name })</li>
    }
</ul>

